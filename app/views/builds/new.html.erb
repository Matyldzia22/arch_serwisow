<h1>New Build</h1>

<%= form_for :build, url: builds_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :country %><br>
    <%= f.text_area :country %>
  </p>

   <p>
    <%= f.label :style %><br>
    <%= f.text_area :style %>
  </p>

   <p>
    <%= f.label :photo %><br>
    <%= f.text_area :photo %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', builds_path %>