<h1>All builds :</h1>
 <%= link_to 'New build', new_build_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Country</th>
    <th>Style</th>
    <th>Photo</th>
  </tr>
 
  <% @builds.each do |build| %>
    <tr>
      <td><%= build.name %></td>
      <td><%= build.country %></td>
      <td><%= build.style %></td>
      <td><%= build.photo %></td>
      <td><%= link_to 'Show', build_path(build) %></td>
      <td><%= link_to 'Edit', edit_build_path(build) %></td>
      <td><%= link_to 'Delete', build_path(build),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>